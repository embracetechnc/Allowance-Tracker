  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^4.0.3",
    "@vitest/ui": "^4.0.3",
